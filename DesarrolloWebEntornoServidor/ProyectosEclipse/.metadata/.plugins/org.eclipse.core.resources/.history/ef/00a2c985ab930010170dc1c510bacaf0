package p01;

import p01.utils.TerminalUtils;

public class Main {
	public static void main(String arg[]) {
		TerminalUtils.output("============== Seleccione una opcion ==============");
		TerminalUtils.output("0. Salir:");
		TerminalUtils.output("1. Comparar Numeros");
		TerminalUtils.output("2. Piedra papel tijera");
		TerminalUtils.output("3. NÃºmeros primos");
		int numero = (int) (Math.random()*10+1);
		for(int i = 0; i <= 20; i++) {
			int numComp = (int) (Math.random()*10+1);
			if(numero < numComp) {
				System.out.println("El primer numero " + numero + " es Menor que " + numComp);
			}else if(numero == numComp) {
				System.out.println("El primer numero " + numero + " es Igual " + numComp);
			}else {
				System.out.println("El primer numero " + numero + " es Mayor " + numComp);
			}
		}
		

		
		
	}

}
