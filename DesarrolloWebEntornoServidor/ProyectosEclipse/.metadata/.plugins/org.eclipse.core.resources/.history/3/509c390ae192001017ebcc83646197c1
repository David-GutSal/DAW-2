package p01;

import java.util.ArrayList;
import java.util.List;
import java.util.Scanner;

public class Main {
	public static void main(String arg[]) {
		
		System.out.print("Hola Mundo \n");
		int numero = (int) (Math.random()*10+1);
		System.out.print(numero + "\n");
		List<String> lista = new ArrayList<String>();
		lista.add("AAA");
		lista.add("BBB");
		
		for(int i = 0; i < lista.size(); i++) {
			System.out.println(lista.get(i));
			
		}
		Nombres nombres = Nombres.CCC;
		System.out.println(nombres.CCC);

		for(int i = 0; i <= 20; i++) {
			int numComp = (int) (Math.random()*10+1);
			if(numero < numComp) {
				System.out.println("El primer numero " + numero + " es Menor que " + numComp);
			}else if(numero == numComp) {
				System.out.println("El primer numero " + numero + " es Igual " + numComp);
			}else {
				System.out.println("El primer numero " + numero + " es Mayor " + numComp);
			}
		}
		
		System.out.println("===========Inicia el juego===========");
		int cpuNum = (int) (Math.random()*3+1);
		String cpuSelection;
		Scanner sc = new Scanner(System.in);
		String seleccion = sc.nextLine();
		switch(cpuNum) {
			case 1:
				cpuSelection = "piedra";
			break;
			case 2:
				cpuSelection = "papel";
			break;
			case 3:
				cpuSelection = "tijera";
			break;
		}
		
	}
	public enum Nombres{
		CCC, DDD;
	}

}
