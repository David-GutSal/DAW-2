package p01;

import p01.utils.TerminalUtils;

public class Main {
	public static void main(String arg[]) {
		TerminalUtils.output("============== Seleccione una opcion ==============");
		TerminalUtils.output("0. Salir:");
		TerminalUtils.output("1. Comparar Numeros");
		TerminalUtils.output("2. Piedra papel tijera");
		TerminalUtils.output("4. Números primos");
		TerminalUtils.output("5. Números primos");
		TerminalUtils.output("6. Números primos");
		TerminalUtils.output("7. Números primos");
		TerminalUtils.output("8. Números primos");
		TerminalUtils.output("9. Números primos");
		TerminalUtils.output("10. Números primos");
		
		int selection = TerminalUtils.inputInt();
		switch(selection) {
		case 0:
			break;
		case 1:
			break;
		case 2:
			break;
		case 3:
			break;
		case 4:
			break;
		case 5:
			break;
		case 6:
			break;
		case 7:
			break;
		case 8:
			break;
		case 9:
			break;
		case 10:
			break;
		default:
			break;
		}
		
		int numero = (int) (Math.random()*10+1);
		for(int i = 0; i <= 20; i++) {
			int numComp = (int) (Math.random()*10+1);
			if(numero < numComp) {
				System.out.println("El primer numero " + numero + " es Menor que " + numComp);
			}else if(numero == numComp) {
				System.out.println("El primer numero " + numero + " es Igual " + numComp);
			}else {
				System.out.println("El primer numero " + numero + " es Mayor " + numComp);
			}
		}
		

		
		
	}

}
